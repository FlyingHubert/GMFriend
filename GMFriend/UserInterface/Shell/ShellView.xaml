             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:GMFriend.UserInterface.Shell"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="800" MinHeight="500" MinWidth="700">
